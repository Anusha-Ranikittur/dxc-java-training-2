class Assignment1

{
      public static void main(String[] args)
    {  int a= 10;
        int b= 20;
        int A=a+b;
        int B=a-b;
        int C=a*b;
        double D=a/b;
            System.out.println("sum of a and b is"+A);
        System.out.println("Sub of a and b is"+B);
        System.out.println("Mul of a and b is"+C);
        System.out.println("Div of a and b is"+D);

    }
}
